        elseif game.PlaceId == 17047374266 then -- boss raid realm
            repeat wait() until game.Players.LocalPlayer
            wait(1.5)
            local pingDrops = {
                "Dreadful Bone Mask",
                "Dreadful Bone Armor",
                "Dark Feng Headband",
                "Feng Star",
                "Yhwach Almighty Cloak",
                "Min Starrk Mask",
                "True Hogyoku"
            }
            local function checkPing(checkItem)
                for i, v in pingDrops do
                    if checkItem:find(v) then
                        return true
                    end
                end
                
                return false
            end
            game.Players.LocalPlayer.PlayerGui.ScreenEffects.ChildAdded:Connect(function(child)
                if child.Name == "ItemFrame" then
                    repeat wait() until child:FindFirstChild("TextLabel")
                    print(child.TextLabel.Text)
                --    if table.find(pingDrops, child.TextLabel.Text) then
                    if checkPing(child.TextLabel.Text) == true then
               --         print("ye nigga i did that " .. tostring(checkPing(child.TextLabel.Text)))
                        customwebhook("[automatically voided] " .. child.TextLabel.Text .. "\n**RARE ITEM OBTAINED, ITEM PASSES PING CHECK!**\n@everyone",_G.webhookLink)
                    else
                   --     print("ye nigga i aint did that " .. tostring(checkPing(child.TextLabel.Text)))
                        customwebhook("[automatically voided] " .. child.TextLabel.Text,_G.webhookLink)
                    end
                end
            end)
            game:GetService("RunService").Stepped:Connect(function()
                sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", 10000)
            end)
            local fall = tonumber(workspace.FallenPartsDestroyHeight+50)
            for i=1,40 do
                while wait() do
                    pcall(function()
                        for i,v in next, Workspace.Entities:GetChildren() do
                            if v:IsA("Model") and v:FindFirstChildWhichIsA("Humanoid", true) and v.Name ~= game.Players.LocalPlayer.Name then
                                if isnetworkowner(v.HumanoidRootPart) then 
                                --    print("[debug data] localplayer has ownership")
                                --    customwebhook("[debug data] localplayer has ownership",_G.webhookLink)
                                    local Humanoid = v:FindFirstChildWhichIsA("Humanoid", true)
                                    task.wait()
                                    v.Humanoid.Health = 0
                                    for ii, vv in pairs(v:GetDescendants()) do 
                                        if vv:IsA("BasePart") or vv:IsA("Part")  then 
                                            vv.CFrame = vv.CFrame * CFrame.new(0, fall, 0) 
                                            vv.CFrame = vv.CFrame * CFrame.new(0, fall, 0)
                                            v.Humanoid.Health = 0
                                            v.Humanoid.Health = 0
                                            v.Humanoid.Health = 0
                                            v:BreakJoints()
                                            v.Humanoid.Health = 0
                                            v.Humanoid.Health = 0
                                            wait(0.05)
                                        end
                                    end
                                    if Humanoid.Health <= Humanoid.MaxHealth then
                                        Humanoid.Health = 0 
                                    end
                                end
                            --    print("[debug data] localplayer has ownership")
                            --    customwebhook("[debug data] localplayer has ownership",_G.webhookLink)
                                local Humanoid = v:FindFirstChildWhichIsA("Humanoid", true)
                                task.wait()
                                v.Humanoid.Health = 0
                                for ii, vv in pairs(v:GetDescendants()) do 
                                    if vv:IsA("BasePart") or vv:IsA("Part") then 
                                        vv.CFrame = vv.CFrame * CFrame.new(0, fall, 0) 
                                        vv.CFrame = vv.CFrame * CFrame.new(0, fall, 0)
                                        v.Humanoid.Health = 0
                                        v.Humanoid.Health = 0
                                        v.Humanoid.Health = 0
                                        v:BreakJoints()
                                        v.Humanoid.Health = 0
                                        v.Humanoid.Health = 0
                                        wait(0.05)
                                    end
                                end
                                if Humanoid.Health <= Humanoid.MaxHealth then
                                    Humanoid.Health = 0 
                                end
                            end
                        end
                    end)
                end
            end
        elseif game.PlaceId == 14070029709 or game.PlaceId == 14069122388 then -- soul society, hueco mundo
