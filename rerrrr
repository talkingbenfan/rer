    getgenv().Session = session
    loadstring(game:HttpGet("https://raw.githubusercontent.com/talkingbenfan/flareTS/main/wlOBF"))()
    wait(0.5)
    if newWhitelist() == "lmao" then 
        game:Shutdown() 
        return 
    end
    repeat wait() until game:IsLoaded()
    repeat wait() until game.Players.LocalPlayer
    local request = request or http_request
    local function customwebhook(happen, link)
        request({
            Url = game:GetService("HttpService"):JSONDecode(readfile("flareSave.TYPESOUL")).webLink;
            Method = 'POST',
            Headers = {
                ['Content-Type'] = 'application/json'
            },
            Body = game:GetService('HttpService'):JSONEncode({content = tostring(happen)})
        }); 
    end
        if game.PlaceId == 14069678431 then -- karakura town
            repeat wait() until game:IsLoaded()
            repeat wait() until game.Players.LocalPlayer.Character
            repeat wait() until Workspace.NPCs.RaidBoss.Kisuke
            for i=1,40 do
                wait(0.05)
                if game:GetService("Players").LocalPlayer.PlayerGui.MissionsUI.CreatePartyFrame.CreateParty.Visible == true then
                    game:GetService("Players").LocalPlayer.Character.CharacterHandler.Remotes.PartyCreate:FireServer()
                end
                if Workspace.NPCs.RaidBoss.Kisuke and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("CharacterHandler") then
                    if game.Players.LocalPlayer.PlayerGui.MissionsUI.MainFrame.Visible then
                        if not game.Players.LocalPlayer:FindFirstChild("Kisuke") then
                            Workspace.NPCs.RaidBoss.Kisuke:FindFirstChildWhichIsA("ClickDetector").MaxActivationDistance = math.huge
                            fireclickdetector(Workspace.NPCs.RaidBoss.Kisuke:FindFirstChildWhichIsA("ClickDetector"))
                        else
                            game.Players.LocalPlayer.Kisuke:FireServer("Yes")
                        end
                    end
                end
            end
            wait(4)
            game.Players.LocalPlayer.Character:BreakJoints()
            wait(10)
            for i=1,40 do
                wait(0.05)
                if game:GetService("Players").LocalPlayer.PlayerGui.MissionsUI.CreatePartyFrame.CreateParty.Visible == true then
                    game:GetService("Players").LocalPlayer.Character.CharacterHandler.Remotes.PartyCreate:FireServer()
                end
                if Workspace.NPCs.RaidBoss.Kisuke and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("CharacterHandler") then
                    if game.Players.LocalPlayer.PlayerGui.MissionsUI.MainFrame.Visible then
                        if not game.Players.LocalPlayer:FindFirstChild("Kisuke") then
                            Workspace.NPCs.RaidBoss.Kisuke:FindFirstChildWhichIsA("ClickDetector").MaxActivationDistance = math.huge
                            fireclickdetector(Workspace.NPCs.RaidBoss.Kisuke:FindFirstChildWhichIsA("ClickDetector"))
                        else
                            game.Players.LocalPlayer.Kisuke:FireServer("Yes")
                        end
                    end
                end
            end
            wait(4)
            game.Players.LocalPlayer.Character:BreakJoints()
            wait(10)
            for i=1,300 do
                wait(0.05)
                if game:GetService("Players").LocalPlayer.PlayerGui.MissionsUI.CreatePartyFrame.CreateParty.Visible == true then
                    game:GetService("Players").LocalPlayer.Character.CharacterHandler.Remotes.PartyCreate:FireServer()
                end
                if Workspace.NPCs.RaidBoss.Kisuke and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("CharacterHandler") then
                    if game.Players.LocalPlayer.PlayerGui.MissionsUI.MainFrame.Visible then
                        if not game.Players.LocalPlayer:FindFirstChild("Kisuke") then
                            Workspace.NPCs.RaidBoss.Kisuke:FindFirstChildWhichIsA("ClickDetector").MaxActivationDistance = math.huge
                            fireclickdetector(Workspace.NPCs.RaidBoss.Kisuke:FindFirstChildWhichIsA("ClickDetector"))
                        else
                            game.Players.LocalPlayer.Kisuke:FireServer("Yes")
                        end
                    end
                end
            end
            repeat wait() until game.Players.LocalPlayer.Character
            for i=1,300 do
                if game.Players.LocalPlayer.Character then
                    local serverlist = game:GetService("ReplicatedStorage").Requests.RequestServerList:InvokeServer("Karakura Town")
                    game:GetService("ReplicatedStorage").Requests.RequestCCList:InvokeServer()
                    function teleport()
                        local server
                        local max = math.huge
                        if serverlist then
                            for i, v in next, serverlist do
                                game.Players.LocalPlayer.Character.CharacterHandler.Remotes.ServerListTeleport:FireServer(
                                    "Karakura Town",
                                    v["JobID"]
                                )
                            end
                        end
                    end
                    teleport()
                    wait(4)
                --    game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, game.JobId, game.Players.LocalPlayer) -- rj same server
                game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer) -- rj any server
                end
