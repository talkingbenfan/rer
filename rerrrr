repeat wait() until game.Players.LocalPlayer
if isfile("RUNKISUKEAUTO27.FLARE") then
    Key = getgenv().Key or game:GetService("HttpService"):JSONDecode(readfile("flareSave.TYPESOUL")).Key
    local sea = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name
    getgenv().sea = sea
    getgenv().Session = "yalla"
